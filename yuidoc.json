{
  "name": "Protoboard",
  "description": "Check out the tutorials and the API on the side bar. Go <a href=\"ex/all/index.html\">here</a> for demo.",
  "version": "0.1.0",
  "url": "https://github.com/garysoed/protoboard",
  "colorBackground": "white",
  "colorForeground": "black",

  "colorSectionLightest": "#B8DAB8",
  "colorSectionLight": "#98CC98",
  "colorSectionNormal": "#74B974",
  "colorSectionDark": "#53A553",
  "colorSectionDarkest": "#328F32",

  "colorCodeLightest": "#E6F3CD",
  "colorCodeLight": "#DAEEB1",
  "colorCodeNormal": "#BDD888",
  "colorCodeDark": "#A1C161",
  "colorCodeDarkest": "#82A63B",
  "options": {
    "linkNatives": "true",
    "outdir": "./doc",
    "themedir": "./node_modules/doctheme",
    "paths": [
      "./src",
      "./tutorial"
    ],
    "external": {
      "data": [
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/dom.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/element.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/js.json"
        }
      ]
    },
    "extension": ".html,.js",
    "helpers": ["./node_modules/doctheme/helpers.js"],
    "nocode": true
  }
}