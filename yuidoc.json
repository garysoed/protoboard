{
  "name": "Protoboard",
  "description": "Check out the tutorials and the API on the side bar. Go <a href=\"ex/all/index.html\">here</a> for demo.",
  "version": "0.1.0",
  "url": "https://github.com/garysoed/protoboard",
  "colorMain": "#004000",
  "colorMainLight": "#00C800",
  "colorKeyword": "#808080",
  "colorLink": "gold",
  "colorLinkExternal": "darkorange",
  "options": {
    "linkNatives": "true",
    "outdir": "./doc",
    "themedir": "./node_modules/doctheme",
    "paths": [
      "./src",
      "./tutorial"
    ],
    "external": {
      "data": [
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/dom.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/element.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/js.json"
        }
      ]
    },
    "extension": ".html,.js",
    "helpers": ["./node_modules/doctheme/helpers.js"],
    "nocode": true
  }
}