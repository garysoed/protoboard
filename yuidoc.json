{
  "name": "Protoboard",
  "description": "Check out the tutorials and the API on the side bar. You can play around with the components at the <a href=\"http://garysoed.github.io/protoboard-demo\">demo site</a>.",
  "version": "1.2.0",
  "url": "https://github.com/garysoed/protoboard",
  "colorBackground": "white",
  "colorForeground": "black",
  "colorSectionLightest": "#9AE08F",
  "colorSectionLight": "#6ECA5F",
  "colorSectionNormal": "#4AAE3B",
  "colorSectionDark": "#309620",
  "colorSectionDarkest": "#19760A",
  "colorCodeLightest": "#DAF49C",
  "colorCodeLight": "#C2E66D",
  "colorCodeNormal": "#9FC643",
  "colorCodeDark": "#82AA24",
  "colorCodeDarkest": "#62870C",
  "options": {
    "linkNatives": "true",
    "outdir": "./doc",
    "themedir": "./node_modules/doctheme",
    "paths": [
      "./src",
      "./tutorial"
    ],
    "external": {
      "data": [
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/dom.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/element.json"
        },
        {
          "base": "../assets/redirect.html#",
          "json": "./node_modules/doctheme/externals/js.json"
        }
      ]
    },
    "extension": ".html,.js",
    "helpers": [
      "./node_modules/doctheme/helpers.js"
    ],
    "nocode": true
  }
}
